@article{bejar_sindicatos_1994,
  title = {Los Sindicatos En {{México}} Ante La Globalización},
  author = {Béjar, Alejandro Álvarez},
  date = {1994},
  journaltitle = {Revista Mexicana de Sociología},
  volume = {56},
  pages = {89--103},
  publisher = {{[Instituto de Investigaciones Sociales de la Universidad Nacional Autónoma de México, Universidad Nacional Autónoma de México]}},
  issn = {0188-2503},
  doi = {10.2307/3540894},
  url = {https://www.jstor.org/stable/3540894},
  urldate = {2020-05-13},
  abstract = {Ante la globalización, el papel social de los sindicatos está severamente cuestionado, en unos casos por la acción proempresarial del Estado, en otros por la voracidad depredadora del propio capital; está además cuestionado por las propias bases trabajadoras y, pese a todo, es evidente que el control corporativo de las organizaciones sindicales sigue siendo el pivote central de la estructura de dominación que tiene el capital sobre las clases subalternas. /// In the face of globalization, the role of trade unions in society is being called into question by the State's pro-managerial actions and the limitless voracity of capital itself. It is also being queried by workers, yet despite this, it is clear that corporate control of trade union organizations continues to be the main pivot of the capital's system of domination over the subordinate classes.},
  number = {1}
}

