@article{zapata_crisis_1994,
  title = {¿{{Crisis}} Del Sindicalismo En {{México}}?},
  author = {Zapata, Francisco},
  date = {1994},
  journaltitle = {Revista Mexicana de Sociología},
  volume = {56},
  pages = {79--88},
  publisher = {{[Instituto de Investigaciones Sociales de la Universidad Nacional Autónoma de México, Universidad Nacional Autónoma de México]}},
  issn = {0188-2503},
  doi = {10.2307/3540893},
  url = {https://www.jstor.org/stable/3540893},
  urldate = {2020-05-13},
  abstract = {El control sindical sobre diversos aspectos de la vida del trabajo en México se encuentra en una encrucijada; las consecuencias estructurales de la caída de los salarios, la compresión de las prestaciones sociales y la represión del conflicto sindical han redundado en un aumento de la explotación del trabajo, la transformación de los mercados internos de trabajo, la flexibilización de las normas productivas y el debilitamiento del sindicato como institución reguladora de la vida fabril. /// Trade unionist control of various aspects of labor in Mexico is currently facing a crossroads: structural consequences of the drop in salaries, reduced social benefits and the repression of the trade unionist conflict have led to increased exploitation at work, transformation of internal labor markets, increased flexibility of production regulations and the weakening of the trade union's role in regulating factory life.},
  number = {1}
}

